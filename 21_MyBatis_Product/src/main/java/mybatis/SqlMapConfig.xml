<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
 
<configuration>

	<typeAliases>
		<typeAlias alias="MyProduct" type="product.model.ProductBean"/>
		<typeAlias alias="MyMember" type="member.model.MemberBean"/>
		<typeAlias alias="MyOrder" type="order.model.OrderBean"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/product.xml"/>
		<mapper resource="mybatis/member.xml"/>
		<mapper resource="mybatis/order.xml"/>
		<mapper resource="mybatis/orderdetail.xml"/>
		<mapper resource="mybatis/composite.xml"/>
	</mappers>
	
</configuration>
 
