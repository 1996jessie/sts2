@ModelAttribute
@Controller
@Valid
@RequestMapping
@Length
@Size
@Autowired
@Qualifier
@Component
@Pattern
@RequestParam
@NotNull
@NotEmpty
@NotBlank
@Override
@WebServlet
@WebInitParam
--------------------------

drop sequence person_seq;
create sequence person_seq
start with 1
minvalue 1
increment by 1;

drop table person;
create table person(
	num number primary key,
	id varchar2(10),
	name varchar2(20),
	age number
);

commit;

select * from person;

